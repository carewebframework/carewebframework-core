<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

	<beans profile="root">
		<!-- Register the help set factory for the Oracle Help format. -->
		<bean factory-method="register" class="org.carewebframework.help.HelpSetFactory">
			<constructor-arg value="org.carewebframework.help.ohj.HelpSet_OHJ" />
			<constructor-arg value="ohj" />
		</bean>
	</beans>

</beans>