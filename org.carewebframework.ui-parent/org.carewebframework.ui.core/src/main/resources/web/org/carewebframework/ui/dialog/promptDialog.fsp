
<window closable="true" class="cwf-promptdialog"
	controller="org.carewebframework.ui.dialog.PromptDialog">
	<span>
		<span name="icon" class="glyphicon" />
		<cell name="message" />
		<div>
			<listbox name="listbox" visible="false" />
			<checkbox name="chkRemember" visible="false"
				label="${@msg.cwf.prompt.chx.reminder.label}" />
		</div>
	</span>
	<toolbar name="toolbar" alignment="center" width="100%" />
</window>
