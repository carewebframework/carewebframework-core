<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:cwp="http://www.carewebframework.org/schema/plugin" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.carewebframework.org/schema/plugin
		http://www.carewebframework.org/schema/plugin/plugin-extensions.xsd">

	<beans profile="root">

		<cwp:plugin id="infoPanelTestPlugin" name="Information Panel Test"
			url="web/org/carewebframework/plugin/infopanel/infoPanelTest.fsp">
		</cwp:plugin>

	</beans>

	<beans profile="child">

		<bean id="infoPanelTestController"
			class="org.carewebframework.plugin.infopanel.InfoPanelTestController"
			scope="prototype">
		</bean>

	</beans>
</beans>
